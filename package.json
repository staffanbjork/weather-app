{
  "name": "weather-app",
  "description": "Weather web application, fullstack made with node, express, sqlite and react.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Staffan Bj√∂rk",
  "scripts": {
    "backend:compile": "cd backend && tsc",
    "backend:watch": "cd backend && tsc --watch",
    "backend:run": "nodemon --env-file=.env backendDist/index.js",
    "start": "npm run backend:compile && concurrently \"npm run backend:watch\" \"npm run backend:run\" \"npm run dev\"",
    "dev": "cd frontend && vite",
    "build": "cd frontend && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "cd frontend && vite preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.16",
    "axios": "^1.7.7",
    "better-sqlite3": "^11.3.0",
    "bootstrap": "^5.3.3",
    "express": "^4.21.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "three": "^0.171.0",
    "vanta": "^0.5.24"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@tanstack/react-query-devtools": "^5.59.16",
    "@types/better-sqlite3": "^7.6.11",
    "@types/express": "^5.0.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-router": "^5.1.20",
    "@types/three": "^0.171.0",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^9.0.1",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "nodemon": "^3.1.7",
    "sass-embedded": "^1.79.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
